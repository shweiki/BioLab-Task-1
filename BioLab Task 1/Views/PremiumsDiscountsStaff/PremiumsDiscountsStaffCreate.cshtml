@model List<POCO.PremiumsDiscountsStaff>
<div style="width: 100%;" class="row">
    <div class="form-group col-md-12">
        <table class="table table-striped table-bordered" id="editable-datatable">
            <thead>
                <tr>
                    <th>Emp no. @Model.Count</th>
                    <th>Emp. Name</th>
                    <th>Desc</th>
                    <th>From Date</th>
                    <th>To Date</th>
                    <th>Value</th>
                </tr>
            </thead>
            <tbody>
                @if (Model.Count > 0)
                {
                    foreach (POCO.PremiumsDiscountsStaff item in Model)
                    {

                        <tr id="Row_@item.empno">
                            <td>@item.empno</td>
                            <td>@item.FullAnname</td>
                            <td>@item.desc</td>
                            <td>@item.fromdate</td>
                            <td>@item.todate</td>
                            <td>@item.value</td>
                        
                        </tr>
                    }
                }
                else
                {
                    <tr> <td class="m-0 p-0" colspan="20"><div class="alert alert-warning text-center m-0">There is no result found.</div></td></tr>
                }
            </tbody>

        </table>
    </div>
</div>
